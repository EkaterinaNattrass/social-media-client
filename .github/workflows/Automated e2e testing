on: 
  pull_request:
  workflow_dispatch:

jobs:
  run-e2e-tests:
    name: Run E2E Tests
    runs-on: ubuntu-latest
    steps:
      - name: Checkout
        uses: actions/checkout@main

      - name: Cypress run
        uses: cypress-io/github-action@v6

       with:
          build: npm run build
          start: npm start
     
